{{ form_start(form) }}

    <form>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Le nom du Pub</label>
        <div class="col-sm-10">
            {{ form_widget(form.namePub) }}
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Adresse</label>
        <div class="col-sm-10">
            {{ form_widget(form.addressPub) }}
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Code postal</label>
        <div class="col-sm-10">
            {{ form_widget(form.cpPub) }}
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Ville</label>
        <div class="col-sm-10">
            {{ form_widget(form.cityPub) }}
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Téléphone</label>
        <div class="col-sm-10">
            {{ form_widget(form.phonePub) }}
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Image du Pub</label>
        <div class="col-sm-10">
            {{ form_widget(form.imgPub) }}
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Description</label>
        <div class="col-sm-10">
            {{ form_widget(form.contentPub) }}
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Horaires</label>
        <div class="col-sm-10">
            {{ form_widget(form.schedulePub|raw) }}
        </div>
    </div>
    <div class="form-group row">
        <label class="col-sm-2 col-form-label">la carte PDF</label>
        <div class="col-sm-10">
            {{ form_widget(form.cardPdf|raw) }}
        </div>
    </div>

    <div class="form-group row">
        <label class="col-sm-2 col-form-label">Localisation google maps</label>
        <div class="col-sm-10">
            {{ form_widget(form.iframePub|raw) }}
        </div>
    </div>

  </form>
    <button class="btn btn-success">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
